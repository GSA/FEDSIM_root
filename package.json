  },
  "dependencies": {
    "gem": "^2.4.3",
    "gems": "^0.0.1",
    "netlify-cms": "^2.10.61",
    "ruby": "^0.6.1",
    "uswds": "^2.13.1"
  },
  "devDependencies": {
    "@uswds/compile": "^1.0.0-alpha.0",
    "a11y": "^0.5.1",
    "rimraf": "^3.0.2"
  }
}
